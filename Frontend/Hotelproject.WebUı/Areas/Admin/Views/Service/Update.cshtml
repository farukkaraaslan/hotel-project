@model UpdateServiceModel
@{
    ViewData["Title"] = "Update";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
   
    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <label>Icon</label>
        <input type="text" class="form-control input-default" asp-for="Icon">
    </div>
    <div class="form-group">
         <label>Başlık</label>
        <input type="text" class="form-control input-default" asp-for="Title">
    </div> 
    <div class="form-group">
         <label>Açıklama</label>
        <input type="text" class="form-control input-default" asp-for="Decription">
    </div>

   <button class="btn btn-primary">Kaydet</button>
</form>

